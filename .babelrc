{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current" // or '7.10.0'
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/proposal-object-rest-spread", false, "transform-rest-spread"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "prisma": "./server/prisma",
          "models": "./server/models",
          "config": "./server/config"
        }
      }
    ]
  ]
}
