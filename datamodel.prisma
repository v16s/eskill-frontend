type User {
  username: String!
  password: String!
  name: String!
  campus: String!
  department: String!
  dob: DateTime
  email: String!
  level: Int!
  id: ID! @id
}
type Global {
  departments: [String]
  campuses: [String]
  regs: Boolean @default(value: true)
  regf: Boolean @default(value: true)
  id: String! @default(value: "global") @unique
  _id: ID! @id
}
