type User {
  username: String! @unique
  password: String!
  name: String!
  campus: String
  department: String
  dob: DateTime
  email: String!
  level: Int!
  id: ID! @id
}
type Global {
  regs: Boolean @default(value: true)
  regf: Boolean @default(value: true)
  id: String! @default(value: "global") @unique
  _id: ID! @id
}
type Tag @embedded {
  name: String!
  id: String!
}
type Campus {
  id: ID! @id @unique
  admin_id: String!
  departments: [Tag]
  name: String! @unique
}
type Branch {
  id: ID! @id
  name: String! @unique
}
type Course {
  id: ID! @id
  branch: String!
  coordinator_id: String!
  name: String! @unique
}
type QuestionAdd {
  id: ID! @id
  course: String!
  name: String!
  desc: String!
  exp: String!
  opt: [Obj]!
  ans: String!
}
type Obj @embedded{
    opt1: String!
    opt2: String!
    opt3: String!
    opt4: String!
    id: String!
}