type User {
  username: String! @unique
  password: String!
  name: String!
  campus: String
  department: String
  dob: DateTime
  email: String!
  level: Int!
  id: ID! @id
}
type Global {
  regs: Boolean @default(value: true)
  regf: Boolean @default(value: true)
  id: String! @default(value: "global") @unique
  _id: ID! @id
}
type Tag @embedded {
  name: String!
  id: String!
}
type Campus {
  id: ID! @id @unique
  admin_id: String!
  departments: [Tag] @relation(name: "CampusDepartmentRelation")
  branches: [Branch] @relation(name: "CampusBranchRelation")
  name: String! @unique
}
type Branch @embedded {
  name: String!
  courses: [Course] @relation(name: "BranchCourseRelation")
}
type Course @embedded {
  coordinator_id: String!
  name: String!
}
